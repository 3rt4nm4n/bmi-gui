<Window x:Class="LabWork1.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork1"
        mc:Ignorable="d"
        Title="Be Healthy Web" Height="500" Width="750">
    <Grid>
        <Button x:Name="showWebPageButton" Content="Show Web Page" HorizontalAlignment="Left" Height="48" Margin="10,39,0,0" VerticalAlignment="Top" Width="200" RenderTransformOrigin="-0.138,0.227" Click="showWebPageButton_Click"/>
        <Button x:Name="backToMenuButton" Content="Back to Menu" HorizontalAlignment="Left" Height="48" Margin="216,39,0,0" VerticalAlignment="Top" Width="200" Click="backToMenuButton_Click"/>
        <WebBrowser HorizontalAlignment="Center" Height="340" Margin="10,120,10,0" VerticalAlignment="Top" Width="724" Source="https://www.who.int/behealthy"/>

    </Grid>
</Window>
